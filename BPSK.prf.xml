<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="Output Rate" mode="readonly" name="Output Rate" type="float">
    <description>The output rate of the demod. This is a read-only property that is calculated based on the input clock's symbol rate.</description>
    <value>0</value>
    <units>S/s</units>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="Symbol Length" mode="readonly" name="Symbol Length" type="long">
    <description>This is a read-only property that shows the bits per symbol of input data that is to be BPSK demodulated. This is calculated from the input sampling rate and the clock's symbol rate. </description>
    <value>0</value>
    <units>bits</units>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
